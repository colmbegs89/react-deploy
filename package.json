{
  "name": "myapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.16.2",
    "http-server": "^0.11.1",
    "morgan": "^1.9.0",
    "pushstate-server": "^3.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^3.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0",
    "react-twitter-embedded-timeline": "^0.5.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "enzyme": "^2.9.1",
    "react-test-renderer": "^15.6.2",
    "sinon": "^3.3.0",
    "standard": "^10.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/coverage/**",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!**/stub_data/**",
      "!**src/setupTests.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  },
  "standard": {
    "globals": [
      "describe",
      "fetch",
      "it",
      "URL"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
